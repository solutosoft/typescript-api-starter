    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/email-templates": "^8.0.1",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.5",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/luxon": "^1.26.5",
    "@types/mysql": "^2.15.17",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
    "jwt-decode": "^3.1.2",
    "supertest": "^6.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>/tests/setup.ts"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/migrations/",
      "/lib/"
    ]
