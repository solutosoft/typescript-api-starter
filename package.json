    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.3",
    "gulp-typescript": "^6.0.0-alpha.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.27.0",
    "mysql": "^2.18.1",
    "nocache": "^3.0.1",
    "nodemailer": "^6.4.17",
    "pug": "^3.0.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
